{
    "name": "todo",
    "version": "1.0.0",
    "description": "Réalisation d‘une Todolist en vue.js avec Nuxt et Vuex",
    "author": "WanFoxOne <contact@corler.pro>",
    "private": true,
    "scripts": {
        "dev": "concurrently --kill-others \"nuxt\" \"yarn api\"",
        "build": "nuxt build",
        "start": "concurrently --kill-others \"nuxt start\" \"yarn api\"",
        "now-start": "nuxt start",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "api": "node_modules/.bin/json-server --watch __api/db.json --port 3001"
    },
    "dependencies": {
        "@nuxtjs-ext/bulma-extensions": "^1.0.0",
        "@nuxtjs/axios": "^5.3.1",
        "@nuxtjs/bulma": "^1.2.0",
        "json-server": "^0.14.0",
        "moment": "^2.22.2",
        "nuxt": "^1.0.0",
        "uniqid": "^5.0.3",
        "vee-validate": "^2.1.0-beta.9"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.1",
        "concurrently": "^4.0.1",
        "eslint": "^4.15.0",
        "eslint-friendly-formatter": "^3.0.0",
        "eslint-loader": "^1.7.1",
        "eslint-plugin-vue": "^4.0.0",
        "node-sass": "^4.9.3",
        "sass-loader": "^7.1.0"
    }
}
